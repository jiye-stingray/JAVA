package Practice3;
class Person{
	private String name;
	private int age;
	
	public Person(String name, int age) {
		this.name = name;
		this.age  = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
}
class Student extends Person{
	public Student(String name, int age, int number) {
		super(name, age);
		this.number = number;
		
	}

	private int number;

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}
	
}
class ForeignStudents extends Student{
	public ForeignStudents(String name, int age, int number, String country) {
		super(name, age, number);
		this.country;
		
	}

	private String country;

	public String getCountry() {
		return country;
	}

	public void setCountry(String country) {
		this.country = country;
	}
	
}

public class PersonTest {

	public static void main(String[] args) {
		Person p = new Person("길동이",22);
		Student s = new Student("황진이",23,100);
		ForeignStudents f = new ForeignStudents("Amy",30,200,"U.S.A");
	
	}


}
