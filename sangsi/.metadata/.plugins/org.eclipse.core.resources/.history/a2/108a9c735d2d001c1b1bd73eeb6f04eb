package sangsi;

class Sports{
	private String name;
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	void rule() {
		System.out.println("규칙");
	}
}

class BaseBall extends Sports{
	int player = 9;
	
	void rule() {
		System.out.println("야구 규칙을 따른다");
	}
}
class PingPong extends Sports{
	String team = "복식";
}

