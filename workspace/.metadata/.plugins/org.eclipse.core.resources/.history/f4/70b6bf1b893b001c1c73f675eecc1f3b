package chap01;
class School extends Object{
	
	int Add(int a, int b) {
		return a + b;
	}
	
	
	
	public String toString() {
		return "박지예";
	}
}

public class ObjectEx {

	public static void main(String[] args) {
		School s = new School();
		
		System.out.println("내이름은 "+s.toString());
		System.out.println(s.Add(1, 2));

	}

}
